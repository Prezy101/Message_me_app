<div class="ui large menu">
  <div class="ui container">

  <a class="active item" href=" <%= root_path %>"> Chartroom
  </a>
  <a class="item">
    Messages
  </a>
  <div class="right menu">
    <div class="ui dropdown item">
      Account <i class="dropdown icon"></i>
      <div class="menu">
        <a class="item" href=" <%= login_path %>">Login</a>
        <% if logged_in? %>
            <%=	 link_to "Logout", logout_path, method: :delete, class: "item" %>
        <% else %>
        <a class="item" href=" <%= root_path %>">Sign Up</a>
        <% end %>
      </div>
    </div>
    <% if !logged_in? %>
  <div class="item">
    <%=	 link_to "Log in", login_path, class: "ui primary button" %>
    </div>
    <% end %>
      </div>
  </div>
</div>
